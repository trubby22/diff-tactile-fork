{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "psb120 debug current file (/tasks/)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/difftactile/tasks",
        },
        {
            "name": "psb120 debug object_repose",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/difftactile/tasks/object_repose.py",
            "console": "integratedTerminal",
            "args": "--use_tactile --use_state",
            "cwd": "${workspaceFolder}/difftactile/tasks",
        }
    ]
}